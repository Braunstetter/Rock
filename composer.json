{
  "name": "braunstetter/rock-bundle",
  "type": "symfony-bundle",
  "description": "A base bundle for the rock.systems ecosystem",
  "license": "MIT",
  "authors": [
    {
      "name": "Slowwie",
      "email": "michaelbrauner82@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/security-bundle": "^5.3",
    "symfony/asset": "^5.3",
    "braunstetter/control-panel-bundle": "^0.2",
    "braunstetter/menu-bundle": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "Rock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rock\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/test-pack": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "nyholm/symfony-bundle-test": "^1.8"
  }
}
