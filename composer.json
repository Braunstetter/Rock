{
    "name": "braunstetter/rock-bundle",
    "type": "symfony-bundle",
    "description": "A base bundle for the rock.systems ecosystem",
    "license": "MIT",
    "authors": [
        {
            "name": "Slowwie",
            "email": "michaelbrauner82@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/security-bundle": "^5.3",
        "symfony/asset": "^5.3",
        "braunstetter/control-panel-bundle": "@dev",
        "braunstetter/menu-bundle": "@dev",
        "braunstetter/plugin-bundle": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Rock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rock\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../ControlPanelBundle/Bundle"
        },
        {
            "type": "path",
            "url": "../MenuBundle"
        },
        {
            "type": "path",
            "url": "../PluginBundle"
        }
    ],
    "require-dev": {
        "symfony/framework-bundle": "^4.4|^5.0",
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0"
    }
}
